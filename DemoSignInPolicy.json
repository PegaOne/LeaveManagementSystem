{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect":"Allow",
            "Action":[
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
                ],
                "Resource":"arn:aws:logs:*:*:*"
        },
        {
            "Effect":"Allow",
            "Action":[
                "cognito-identity:*",
                "cognito-idp:*",
                "cognito-sync:*",
                "iam:ListRoles",
                "iam:ListopenIdConnectProviders",
                "sns:ListPlatformApplications"
                ],
                "Resource":"*"
        }
        ]
}
